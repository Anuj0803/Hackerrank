// find the nth smallest no. in an array
package practice;
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) 
    {
    	int s=0;
    	Scanner sc = new Scanner(System.in);
    	int n =sc.nextInt();
    	int arr[]=new int[n];
    	for(int i=0;i<n;i++)
    	{
    		 arr[i] = sc.nextInt();
    	}
    	int p = sc.nextInt();
    	Arrays.parallelSort(arr);
    	System.out.println(arr[p-1]);
    }
}
